
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="~/Scripts/jquery-3.7.0.min.js"></script>

<button class="btn btn-primary" id="btn1">Button1</button>
<button class="btn btn-warning" id="btn2">Button2</button>
<button class="btn btn-danger" id="btn3">Button3</button>

<script>
    $('#btn1').click(function () {
        Swal.fire({
            title: "Merhaba Mvc Proje Kampı",
            icon: "success",
        });
    });
</script>
<script>
    $('#btn2').click(function () {
        Swal.fire({
            position: "top-end",
            icon: "success",
            title: "Mvc Proje Kampı",
            text:"Ders 96",
            showConfirmButton: false,
            timer: 1500
        });
    });
</script>
<script>
    $('#btn3').click(function () {
        Swal.fire({
            title: "Bilgi",
            text: "Mvc Proje Kampı Hakkında",
            icon: "question"
        });
    });
</script>